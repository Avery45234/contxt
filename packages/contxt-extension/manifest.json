{
  "manifest_version": 3,
  "name": "contxt (Stable Dev)",
  "version": "0.1.0",
  "description": "Provides context for news articles.",
  "permissions": [
    "sidePanel"
  ],
  "background": {
    "service_worker": "src/background/index.ts",
    "type": "module"
  },
  "action": {
    "default_title": "contxt"
  },
  "side_panel": {
    "default_path": "sidebar.html"
  }
}